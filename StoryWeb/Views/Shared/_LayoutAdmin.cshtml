<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>@ViewBag.Title - StoryWeb Admin</title>
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/Admin/css/bootstrap.min.css")">
    <link rel="stylesheet" type="text/css" href="@Url.Content("~/Content/Admin/css/style.css")">
    <link rel="stylesheet" href="@Url.Content("~/Content/Admin/css/responsive.css")">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
</head>
<body>
    <div class="container-fluid">
        <div class="content mt-4">
            @RenderBody()
        </div>
    </div>

    <script src="@Url.Content("~/Content/Admin/js/jquery.min.js")"></script>
    <script src="@Url.Content("~/Content/Admin/js/bootstrap.bundle.min.js")"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>